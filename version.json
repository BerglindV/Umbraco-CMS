{
  "$schema": "https://raw.githubusercontent.com/dotnet/Nerdbank.GitVersioning/master/src/NerdBank.GitVersioning/version.schema.json",
  "version": "10.8.10",
  "assemblyVersion": {
    "precision": "build"
  },
  "gitCommitIdShortFixedLength": 7,
  "nuGetPackageVersion": {
    "semVer": 2.0
  },
  "publicReleaseRefSpec": [
    "^refs/heads/main$",
    "^refs/heads/release/"
  ],
  "cloudBuild": {
    "buildNumber": {
      "enabled": true
    }
  }
}
